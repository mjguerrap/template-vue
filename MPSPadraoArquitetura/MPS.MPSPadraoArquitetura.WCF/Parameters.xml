<?xml version="1.0" encoding="utf-8"?>
<parameters>
  <!-- connectionStrings -->
  <parameter name="DefaultConnection" defaultValue="__DefaultConnection__">
    <parameterEntry kind="XMLFile" match="/configuration/connectionStrings/add[@name='DefaultConnection']/@connectionString" scope="\\web.config$"/>
  </parameter>
  <!-- appSettings -->
  <parameter name="Release" defaultvalue="__Release.ReleaseName__">
    <parameterentry kind="XmlFile" match="/configuration/appSettings/add[@key='Release']/@value" scope="\\web.config$" />
  </parameter>
  <parameter name="ChaveBanco" defaultvalue="__ChaveBanco__">
    <parameterentry kind="XmlFile" match="/configuration/appSettings/add[@key='ChaveBanco']/@value" scope="\\web.config$" />
  </parameter>
</parameters>